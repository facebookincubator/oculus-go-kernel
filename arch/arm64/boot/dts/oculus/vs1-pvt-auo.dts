/dts-v1/;

#include "vs1-dvt.dtsi"

/ {
	model = "vs1 pvt (AUO)";
	compatible = "qcom,apq8998-mtp", "qcom,apq8998", "qcom,mtp";
	qcom,board-id = <0x08 0x0F>;
};

&panel_avdd_regulator {
	ocp-gpio = <&tlmm 125 0>;
};

&pmi8998_wled {
	qcom,avdd-target-voltage-mv = <6100>;
};
